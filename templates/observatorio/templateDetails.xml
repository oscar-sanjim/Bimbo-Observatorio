<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "https://www.joomla.org/xml/dtd/1.6/template-install.dtd">
<extension version="3.1" type="template" client="site">
	<name>Bimbo Observatorio</name>
	<creationDate>August 2020</creationDate>
	<author>Oscar SÃ¡nchez</author>
	<authorEmail>oscar.sanjim@gmail.com</authorEmail>
	<authorUrl></authorUrl>
	<copyright>Copyright (C) 2005 - 2020 Open Source Matters, Inc. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<version>3.1.0</version>
	<description>Template of the Observatorio custom tool</description>

	<files>
		<folder>css</folder>
		<folder>js</folder>
		<folder>assets</folder>
		<folder>libs</folder>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>favicon.ico</filename>
		<filename>component.php</filename>
		<filename>error.php</filename>
	</files>

	<positions>
	</positions>

	<!-- 	For core templates, we also install/uninstall the language files in the core language folders.
	-->
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.tpl_beez3.ini</language>
		<language tag="en-GB">en-GB/en-GB.tpl_beez3.sys.ini</language>
	</languages>

	<config>
		<fields name="params">
			<fieldset name="advanced">
				<field name="layout" type="list" default="login" label="Layout" description="" filter="word">
					<option value="login">Login</option>
					<option value="home">Home</option>
				</field>
			</fieldset>
		</fields>
	</config>
</extension>
